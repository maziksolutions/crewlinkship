                    <div class="media-body pt-5 px-2">
                        <div class="row">
                            <div class="col">
                                <h3 class="card-title m-b-0" style="font-size: 1.4rem;">Amarjit Singh Banka <span class="alert bg-success p-t-3 p-b-3 font-small-3 position-relative position-tm-3">On Board</span></h3>
                                <div>Master | BAKERLOO</div>
                            </div>
                        </div>
                    </div>
                </div>
                <nav class="navbar navbar-profile align-self-end m-r-30">
                    <div id="navigation">
                        <ul>
                            <li class="has-sub active">
                                <a >Personal Details</a>
                                <ul>
                                    <li><a class="details" href="javascript:;">Particulars</a></li>
                                    <li><a class="addressdetails" href="javascript:;">Address</a></li>
                                    <li><a class="Bankdetails" href="javascript:;">Bank Details</a></li>

                                </ul>
                            </li>
                            <li class="has-sub">
                                <a>Documents</a>
                                <ul>
                                    <li><a class="license" href="javascript:;">License</a></li>
                                    <li><a class="courses" href="javascript:;">Courses</a></li>
                                    <li><a class="otherDocuments" href="javascript:;">Other Documents</a></li>
                                </ul>
                            </li>
                            <li><a class="crewtraveldoc" href="javascript:;">Travel Documents</a></li>
                            @*<li class="icon" data-toggle="tooltip" data-trigger="hover" data-placement="top" data-title="Activity Log" data-original-title="" title=""><a href=""><i class="fa fa-file" aria-hidden="true"></i></a></li>*@
                        </ul>
                    </div>
                </nav>
            </div>
        </div>
    </div>
</div>
<div id="partialModal" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header  navbar-dark">
                <h5 class="modal-title">
                    <img class="brand-logo" alt="stack admin logo" src="app-assets/images/logo/Crewlink-logo-light.png">
                </h5>
                <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
@*Particulars*@
<script type="text/javascript">
    $(function () {
        $("#navigation .details").click(function () {
            var customerId = $(this).closest("tr").find("td").eq(0).html();
            $.ajax({
                type: "POST",
                url: "/Home/Details",
                data: '{customerId: "' + customerId + '" }',
                contentType: "application/json; charset=utf-8",
                dataType: "html",
                success: function (response) {
                    $("#partialModal").find(".modal-body").html(response);
                    $("#partialModal").modal('show');
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
    });
</script>


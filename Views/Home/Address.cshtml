<!-- / main menu-->
<div class="app-content content fixed-navbar-inner-crew">
    <div class="content-wrapper">
        <div class="content-header row">
            <div class="col-xl-6 col-lg-12">
                <h4 class="content-header-title m-b-10 m-t-5">Address</h4>
            </div>
            <div class="col-xl-6 col-lg-12 nopl">
            </div>
        </div>
        <div class="content-body">
            <div class="row match-height" id="switch">
                <div class="col-xl-6 col-lg-12">
                    <section id="descriptioin" class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-xl-6 col-lg-12">
                                    <h4 class="card-title p-t-5 proper-case">Permanent</h4>
                                </div>
                                <div class="col-xl-6 col-lg-12">
                                    <div class="float-right">
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body pt-0">
                            <div class="table-responsive">
                                <table class="table">
                                    <tbody>

                                        <tr>
                                            <th width="50%">Flat, House no., Building</th>
                                            <td width="50%">@ViewBag.address.Address1</td>
                                        </tr>
                                        <tr>
                                            <th>Area, Colony, Street, Sector, Village</th>
                                            <td>@ViewBag.address.Address2 </td>
                                        </tr>
                                        <tr>
                                            <th>Country</th>
                                            <td>@ViewBag.address.Country?.CountryName</td>
                                        </tr>
                                        <tr>
                                            <th>State</th>
                                            <td>@ViewBag.address.State?.StateName</td>
                                        </tr>
                                        <tr>
                                            <th>Town / City</th>
                                            <td>@ViewBag.address.City?.CityName</td>
                                        </tr>
                                        <tr>
                                            <th>Postcode</th>
                                            <td>@ViewBag.address.Postcode</td>
                                        </tr>
                                        <tr>
                                            <th>Residential Phone</th>
                                            <td>@ViewBag.address.PhoneNumber</td>
                                        </tr>
                                        <tr>
                                            <th>Mobile Number</th>
                                            <td>@ViewBag.address.MobileNumber</td>
                                        </tr>
                                        <tr>
                                            <th>E-mail</th>
                                            <td>@ViewBag.address.Email</td>
                                        </tr>
                                        <tr>
                                            <th>Nearest Airport</th>
                                            <td>@ViewBag.address.Airport?.CityName</td>
                                        </tr>

                                    </tbody>

                                </table>
                            </div>
                        </div>
                    </section>
                </div>
                <div class="col-xl-6 col-lg-12">
                    <section id="descriptioin" class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-xl-6 col-lg-12">
                                    <h4 class="card-title p-t-5 proper-case">Correspondence</h4>
                                </div>
                                <div class="col-xl-6 col-lg-12">


                                </div>
                            </div>
                        </div>
                        <div class="card-body pt-0">
                            <div class="table-responsive">
                                @if (ViewBag.corsAddress != null)
                                {
                                    <table class="table">
                                        <tbody>

                                            <tr>
                                                <th width="50%">Flat, House no., Building</th>
                                                <td width="50%">@ViewBag.corsAddress.Caddress1</td>
                                            </tr>
                                            <tr>
                                                <th>Area, Colony, Street, Sector, Village</th>
                                                <td>@ViewBag.corsAddress.Caddress2</td>
                                            </tr>
                                            <tr>
                                                <th>Country</th>
                                                <td>@ViewBag.corsAddress.Country?.CountryName</td>
                                            </tr>
                                            <tr>
                                                <th>State</th>
                                                <td>@ViewBag.corsAddress.State?.StateName</td>
                                            </tr>
                                            <tr>
                                                <th>Town / City</th>
                                                <td>@ViewBag.corsAddress.City?.CityName</td>
                                            </tr>
                                            <tr>
                                                <th>Postcode</th>
                                                <td>@ViewBag.corsAddress.Postcode</td>
                                            </tr>
                                            <tr>
                                                <th>Nearest Airport</th>
                                                <td>@ViewBag.corsAddress.Airport?.CityName</td>
                                            </tr>

                                        </tbody>
                                    </table>
                                }
                            </div>
                        </div>
                    </section>
                </div>
            </div>
        </div>
    </div>
</div>
<div class="sidenav-overlay"></div>

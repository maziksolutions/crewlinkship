@model IEnumerable<crewlinkship.Models.TblCrewAddress>
@{
    ViewBag.Title = "Address";
}
<nav class="header-navbar navbar-expand-md navbar navbar-with-menu popup-fixed-top fixed-top navbar-dark navbar-shadow navbar-brand-center">
    <div class="navbar-wrapper">
        <div id="user-profile">
            <div class="row">
                <div class="col-12">
                    <div class="card profile-with-cover m-b-0">
                        <div class="card-img-top img-fluid bg-cover height-100" style="background: #E3EBF3"></div>
                        <div class="media profil-cover-details w-100">
                            <div class="media-body pt-5 px-2">
                                <div class="row">
                                    <div class="col">
                                        <h3 class="card-title m-b-0" style="font-size: 1.4rem;">@ViewBag.rankName.FirstName @ViewBag.rankName.MiddleName @ViewBag.rankName.LastName <span class="alert bg-success p-t-3 p-b-3 font-small-3 position-relative position-tm-3">@ViewBag.rankName.Status</span></h3>
                                        <div>
                                            @ViewBag.rankName.Rank?.RankName | @ViewBag.rankName.Vessel?.VesselName <span hidden="hidden" id="crewIdAddress">@ViewBag.rankName.CrewId</span>
                                        </div>

                                    </div>
                                </div>
                            </div>
                        </div>
                        <nav class="navbar navbar-profile align-self-end m-r-30">
                            <div id="navigation">
                                <ul>
                                    <li class="has-sub active">
                                        <a>Personal Details</a>
                                        <ul>
                                            <li><a class="details" href="javascript:;">Particulars</a></li>
                                            <li><a class="address" href="javascript:;">Address</a></li>
                                            <li><a id="Bankdetails" href="javascript:;">Bank Details</a></li>
                                        </ul>
                                    </li>
                                    <li class="has-sub">
                                        <a>Documents</a>
                                        <ul>
                                            <li><a id="license" href="javascript:;">License</a></li>
                                            <li><a id="courses" href="javascript:;">Courses</a></li>
                                            <li><a id="otherDocuments" href="javascript:;">Other Documents</a></li>
                                        </ul>
                                    </li>
                                    <li><a id="crewtraveldoc" href="javascript:;">Travel Documents</a></li>

                                    @*<li class="icon" data-toggle="tooltip" data-trigger="hover" data-placement="top" data-title="Activity Log" data-original-title="" title=""><a href=""><i class="fa fa-file" aria-hidden="true"></i></a></li>*@
                                </ul>
                            </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>
</nav>
<div id="partialModal" class="modal" tabindex="-1" role="dialog">
    <div class="modal-dialog modal-xl" role="document">
        <div class="modal-content">
            <div class="modal-header  navbar-dark">
                <h5 class="modal-title">
                    <img class="brand-logo" alt="stack admin logo" src="app-assets/images/logo/Crewlink-logo-light.png">
                </h5>
                <button type="button" class="close text-white" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary" data-dismiss="modal">Close</button>
            </div>
        </div>
    </div>
</div>@*@Particulars*@
<script type="text/javascript" src="https://ajax.googleapis.com/ajax/libs/jquery/1.8.3/jquery.min.js"></script>
<script type="text/javascript">
    $(function () {
        $("#navigation .details").click(function () {
            var crewId = $("#crewIdAddress").html();
            $.ajax({
                type: "GET",
                url: "/Home/Details",
                data: { 'crewId': crewId },
                contentType: "application/json; charset=utf-8",
                dataType: "html",
                success: function (response) {
                    $("#partialModal").find(".modal-body").html(response);
                    $("#partialModal").modal('show');
                },
                failure: function (response) {
                    alert(response.responseText);
                },
                error: function (response) {
                    alert(response.responseText);
                }
            });
        });
    });
</script>

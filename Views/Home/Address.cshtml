<div>
    <div class="content-wrapper">
        <div class="content-header row">
            <div class="col-xl-6 col-lg-12">
                <h4 class="content-header-title m-b-10 m-t-5">Address</h4>
            </div>
            <div class="col-xl-6 col-lg-12 nopl">
            </div>
        </div>
        <div class="content-body">
            <div class="row match-height" id="switch">
                <div class="col-xl-6 col-lg-12">
                    <section id="descriptioin" class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-xl-6 col-lg-12">
                                    <h4 class="card-title p-t-5 proper-case">Permanent</h4>
                                </div>
                                <div class="col-xl-6 col-lg-12">
                                    <div class="float-right">
                                      
                                    </div>
                                </div>
                            </div>
                        </div>
                        <div class="card-body pt-0">
                            <div class="table-responsive">
                                <table class="table">
                                    <tbody>
                                        @foreach (TblCrewAddress item in ViewBag.address)
                                        {
                                            <tr>
                                                <th width="50%">Flat, House no., Building</th>
                                                <td width="50%">@item.Address1</td>
                                            </tr>
                                            <tr>
                                                <th>Area, Colony, Street, Sector, Village</th>
                                                <td>@item.Address2 </td>
                                            </tr>
                                            <tr>
                                                <th>Country</th>
                                                <td>@item.Country?.CountryName</td>
                                            </tr>
                                            <tr>
                                                <th>State</th>
                                                <td>@item.State?.StateName</td>
                                            </tr>
                                            <tr>
                                                <th>Town / City</th>
                                                <td>@item.City?.CityName</td>
                                            </tr>
                                            <tr>
                                                <th>Postcode</th>
                                                <td>@item.Postcode</td>
                                            </tr>
                                            <tr>
                                                <th>Residential Phone</th>
                                                <td>@item.PhoneNumber</td>
                                            </tr>
                                            <tr>
                                                <th>Mobile Number</th>
                                                <td>@item.MobileNumber</td>
                                            </tr>
                                            <tr>
                                                <th>E-mail</th>
                                                <td>@item.Email</td>
                                            </tr>
                                            <tr>
                                                <th>Nearest Airport</th>
                                                <td>@item.Airport?.CityName</td>
                                            </tr>
                                        }
                                    </tbody>
                                    
                                </table>
                            </div>
                        </div>
                    </section>
                </div>
                <div class="col-xl-6 col-lg-12">
                    <section id="descriptioin" class="card">
                        <div class="card-header">
                            <div class="row">
                                <div class="col-xl-6 col-lg-12">
                                    <h4 class="card-title p-t-5 proper-case">Correspondence</h4>
                                </div>
                                <div class="col-xl-6 col-lg-12">
                                   

                                </div>
                            </div>
                        </div>
                        <div class="card-body pt-0">
                            <div class="table-responsive">
                                <table class="table">
                                    <tbody>
                                        @foreach (TblCrewCorrespondenceAddress item in ViewBag.corsAddress)
                                        {
                                    <tr>
                                        <th width="50%">Flat, House no., Building</th>
                                        <td width="50%">@item.Caddress1</td>
                                    </tr>
                                        <tr>
                                            <th>Area, Colony, Street, Sector, Village</th>
                                            <td>@item.Caddress2</td>
                                        </tr>
                                        <tr>
                                            <th>Country</th>
                                            <td>@item.Country?.CountryName</td>
                                        </tr>
                                        <tr>
                                            <th>State</th>
                                            <td>@item.State?.StateName</td>
                                        </tr>
                                        <tr>
                                            <th>Town / City</th>
                                            <td>@item.City?.CityName</td>
                                        </tr>
                                        <tr>
                                            <th>Postcode</th>
                                            <td>@item.Postcode</td>
                                        </tr>
                                        <tr>
                                            <th>Nearest Airport</th>
                                            <td>@item.Airport?.CityName</td>
                                        </tr>
                                        }
                                    </tbody>
                                </table>
                            </div>
                        </div>
                    </section>
                </div>
            </div>

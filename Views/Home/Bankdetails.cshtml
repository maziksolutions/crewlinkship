                                            <tr>
                                                <th width="50%">Beneficiary</th>
                                                <td width="50%">@item.Beneficiary</td>
                                            </tr>
                                            <tr>
                                                <th>Acc Number</th>
                                                <td>@item.AccountNumber<span hidden="hidden" id="CrewAddId">@item.CrewId</span></td>
                                            </tr>
